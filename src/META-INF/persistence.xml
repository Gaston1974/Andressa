<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="AndressaPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entidades.Inversion</class>
    <class>entidades.ComprasD</class>
    <class>entidades.Stock</class>
    <class>entidades.Articulos</class>
    <class>entidades.MediosDePago</class>
    <class>entidades.Vendedores</class>
    <class>entidades.Clientes</class>
    <class>entidades.Compradores</class>
    <class>entidades.VentasD</class>
    <class>entidades.Proveedores</class>
    <class>entidades.VentasC</class>
    <class>entidades.Localidades</class>
    <class>entidades.ComprasC</class>
    <class>entidades.Banco</class>
    <class>entidades.Dia</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://localhost:5432/Andressa"/>
      <property name="javax.persistence.jdbc.user" value="postgres"/>
      <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
      <property name="javax.persistence.jdbc.password" value="gaston"/>
    </properties>
  </persistence-unit>
</persistence>
